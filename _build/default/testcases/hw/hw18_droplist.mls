type nat =
  | O
  | S of nat

type list =
  | Nil
  | Cons of nat * list

synth list -> nat -> list satisfying

[Nil, 0] -> Nil,
[Nil, 1] -> Nil,
[Cons(4,Nil), 0] -> Cons(4,Nil),
[Cons(1,Cons(2,Cons(3,Nil))), 1] -> Cons(2,Cons(3,Nil)),
[Cons(2,Cons(1,Nil)), 2] -> Nil,
[Cons(3,Cons(5,Nil)), 1] -> Cons(5,Nil),
[Cons(1,Cons(4,Cons(7,Nil))), 2] -> Cons(7,Nil),
