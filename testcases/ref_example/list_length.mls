type nat =
  | O
  | S of nat

type list =
  | Nil
  | Cons of nat * list

synth list -> nat satisfying

[Nil] -> 0,
[Cons(0,Nil)] -> 1,
[Cons(0,Cons(0,Nil))] -> 2,

equiv

fix (f : list -> nat) =
  fun (x:list) -> match x with
                    | Nil -> O
                    | Cons (h,t) -> S (f t)