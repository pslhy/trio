type bool =                                   
  | False                                     
  | True                                      

synth bool -> bool -> bool satisfying

(*[True,True] -> True,*)
[True,False] -> True,
[False,True] -> True,
[False,False] -> False

equiv

fix (f : bool -> bool -> bool) =
  fun (b1:bool) ->
    fun (b2:bool) ->
    match b1 with
      | False -> b2
      | True -> True