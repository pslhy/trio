type nat =
  | O
  | S of nat

type list =
  | Nil
  | Cons of nat * list

synth list -> nat -> list satisfying

[Nil,0] -> Cons(0,Nil),
[Nil,1] -> Cons(1,Nil),
[Cons(2,Cons(1,Cons(0,Nil))),1] -> Cons(2,Cons(1,Cons(0,Cons(1,Nil)))),

equiv

fix (f : list -> nat -> list) =
  fun (l:list) ->
    fun (e:nat) ->
      match l with
        | Nil -> Cons (e, Nil)
        | Cons (h,t) -> Cons (h, f t e)