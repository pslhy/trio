type nat =
  | O
  | S of nat

type bool =
  | True
  | False
type cmp =
  | LT
  | EQ
  | GT

let compare =
  fix (compare : nat -> nat -> cmp) =
    fun (x1 : nat) ->
      fun (x2 : nat) ->
        match x1 with
        | O -> (match x2 with
                | O -> EQ
                | S _ -> LT)
        | S x1 -> (match x2 with
                | O -> GT
                | S x2 -> compare x1 x2)
;;

synth nat -> nat -> nat satisfying

[0,0] -> 0,
[0,1] -> 1,
[0,2] -> 2,
[1,0] -> 1,
[1,1] -> 1,
[1,2] -> 2,
[2,0] -> 2,
[2,1] -> 2,
[2,2] -> 2

equiv

fix (f : nat * nat -> nat) =
  fun (n1:nat) ->
    fun (n2:nat) ->
      match compare n1 n2 with
        | LT _ -> n2
        | EQ _ -> n1
        | GT _ -> n1